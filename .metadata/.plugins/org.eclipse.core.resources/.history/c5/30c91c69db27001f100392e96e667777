package Program;

import java.util.HashMap;
import java.util.Map;

class Sort
{
	Map<String, Integer> map=new HashMap();
	
	public void input()
	{
		map.put("Student1", 31);
		map.put("Student2", 2);
		map.put("Student3", 87);
		map.put("Student4", 43);
		map.put("Student5", 78);
		
		map.forEach((key, value)-> System.out.println(key +" "+ value));
	}
}

