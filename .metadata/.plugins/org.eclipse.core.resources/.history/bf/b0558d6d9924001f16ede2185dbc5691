package Program;

class DeadLock implements Runnable
{
	String s1=new String("JAVA");
	String s2=new String("O.S.");
	String s3=new String("D.S.A.");
	
	@Override
	public void run()
	{
		String name=Thread.currentThread().getName();
		
		try
		{
			if(name.equals("Ram"))
			{
				Thread.sleep(2000);
				synchronized (s1)
				{
					System.out.println("Ram acquired "+s1);
					Thread.sleep(2000);
					synchronized (s2)
					{
						System.out.println("Ram acquired "+s2);
						Thread.sleep(2000);
						synchronized (s3) 
						{
							System.out.println("Ram acquired "+s3);
						}
					}
				}
			}
			else
			{
				Thread.sleep(2000);
				synchronized (s3) 
				{
					System.out.println("Shyam acquired "+s3);
					Thread.sleep(2000);
					synchronized (s2) 
					{
						System.out.println("Shyam acqired "+s2);
						Thread.sleep(2000);
						synchronized (s1) 
						{
							System.out.println("Shyam acquired "+s1);
						}
					}
				}
			}
		}
		catch(Exception e)
		{
			System.out.println("Exception occur");
		}
	}
	
	
}

public class Program_1
{
	public static void main(String[] args) 
	{
		
		
		
	}
}