package Program;

class Singleton
{
	static Singleton sc=new Singleton();
	
	private Singleton()
	{
		System.out.println("Singleton Constructor Exceute");
	}
	
	public static Singleton getObj()
	{
		return sc;
	}
}

public class Program_1
{
	public static void main(String[] args) 
	{
		Singleton sc1=Singleton.getObj();
		Singleton sc2=Singleton.getObj();
		
		System.out.println(sc1.equals(sc2));
		System.out.println(sc1.hashCode());
		System.out.println(sc2.hashCode());
		
		
	}
}